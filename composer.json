{
    "name": "masterarrow/laravel-jira-rest-client",
    "description": "The aim of the package is to make it easier to communicate with the Jira",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitalii Kisielov",
            "email": "masterarrows@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "ext-json": "*",
        "mrjoops/oauth2-jira": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Atlassian\\JiraRest\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atlassian\\JiraRest\\JiraRestServiceProvider"
            ],
            "aliases": {
                "Jira": "Atlassian\\JiraRest\\Facades\\Jira"
            }
        }
    },
    "suggest": {
        "salsify/json-streaming-parser": "Require this when you want to parse the response in blocks"
    }
}
